{
  "name": "@graphai/extra-agents",
  "version": "0.0.1",
  "description": "Agents for GraphAI.",
  "main": "lib/index.js",
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "rm -r lib/* && tsc && tsc-alias",
    "eslint": "eslint",
    "doc": "npx agentdoc",
    "examplesDoc": "npx ts-node  -r tsconfig-paths/register tests/graphai/examples.ts",
    "format": "prettier --write '{src,tests,samples}/**/*.ts'",
    "test": "node --test  -r tsconfig-paths/register --require ts-node/register ./tests/**/test_*.ts",
    "b": "yarn run format && yarn run eslint && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/receptron/agents"
  },
  "author": "Satoshi Nakajima, Isamu Arimoto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/receptron/agents/issues"
  },
  "homepage": "https://github.com/receptron/agents/blob/main/packages/agents/README.md",
  "dependencies": {
    "@graphai/arxiv_agent": "^0.0.2",
    "@graphai/awesome_chatgpt_prompts_agent": "^0.0.1",
    "@graphai/prompts": "^0.0.1",
    "@graphai/serper_agent": "^0.0.1",
    "@graphai/slack_agent": "^0.0.2",
    "@graphai/tts_nijivoice_agent": "^0.0.2",
    "@graphai/tts_openai_agent": "^0.0.2"
  },
  "devDependencies": {},
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  }
}
